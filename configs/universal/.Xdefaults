! URxvt Config

URxvt.depth: 32
URxvt.background: [95]#1E1E1E
URxvt.internalBorder: 0
URxvt.externalBorder: 0

URxvt.geometry: 84x22
!URxvt.geometry: 200x100
URxvt.antialias: true
URxvt.letterSpace: 0

URxvt.font: xft:Hack Nerd Font:size=14:style=Regular
URxvt.boldFont: xft:Hack Nerd Font:size=14:style=Bold
URxvt.italicFont: xft:Hack Nerd Font:size=14:style=Italic
URxvt.boldItalicFont: xft:Hack Nerd Font:size=14:style=Bold Italic

URxvt.scrollBar: false

URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
