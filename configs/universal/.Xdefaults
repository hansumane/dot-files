! URxvt Config

URxvt.depth: 32
URxvt.background: [85]#383c4a
URxvt.internalBorder: 0
URxvt.externalBorder: 0

! URxvt.geometry: 84x22
URxvt.geometry: 200x100
URxvt.antialias: true
URxvt.letterSpace: 0

URxvt.font: xft:JetBrainsMono Nerd Font:size=14:style=Regular
URxvt.boldFont: xft:JetBrainsMono Nerd Font:size=14:style=Medium
URxvt.italicFont: xft:JetBrainsMono Nerd Font:size=14:style=Italic
URxvt.boldItalicFont: xft:JetBrainsMono Nerd Font:size=14:style=Medium Italic

URxvt.scrollBar: false

URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
